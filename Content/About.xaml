<UserControl x:Class="AusTacQuick2Launch.Content.About"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="List" SelectedSource="/Content/AboutApp.xaml">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="About" Source="/Content/AboutApp.xaml" />
                <mui:Link DisplayName="About [AusTac]" Source="/Content/AboutAusTac.xaml" />
                <mui:Link DisplayName="Feedback" Source="/Content/FeedbackForm.xaml" />
                <mui:Link DisplayName="ChangeLog" Source="/Content/ChangeLog.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
